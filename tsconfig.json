{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "lib": [
        "dom",
        "es6"
      ],
      "types": [
        "cypress"
      
      ],
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "importHelpers": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "baseUrl": ".",
      "paths": {
        "*": [
          "cypress/*"
        ]
      }
    },
    "include": [
      "node_modules/cypress",
      "node_modules/chai-subset",
      "cypress",
      "**/*.ts"
    ]
  }